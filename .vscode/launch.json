{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "neoxp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/neoxp/bin/Debug/net6.0/neoxp.dll",
            "cwd": "${workspaceFolder}/src/neoxp",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "neotrace",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/trace/bin/Debug/net6.0/neotrace.dll",
            "args": "block 2190165 -r MainNet",
            "cwd": "${workspaceFolder}/src/trace",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "worknet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/worknet/bin/Debug/net6.0/neo-worknet.dll",
            // "args": "create testnet -f",
            // "args": "prefetch nudes",
            "args": "run -s 1",
            "cwd": "${workspaceFolder}/src/worknet",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}