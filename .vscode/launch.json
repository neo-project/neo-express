{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "neotrace",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/neotrace/bin/Debug/net5.0/neotrace.dll",
            "cwd": "${workspaceFolder}/src/neotrace",
            "stopAtEntry": false,
            "console": "internalConsole"
        },        
        {
            "name": "neoxp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/neoxp/bin/Debug/net5.0/neoxp.dll",
            "cwd": "${workspaceFolder}/src/neoxp",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}