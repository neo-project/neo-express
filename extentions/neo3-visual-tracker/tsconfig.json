{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["esnext", "dom", "dom.iterable"],
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "out",
    "strict": true,
    "jsx": "react",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules", "gulpfile.ts"]
}
